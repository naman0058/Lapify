<%- include ('./adminheader.ejs') %>
      <!-- Main Content -->
      <div class="main-content">
        <section class="section">
          <div class="section-body">
            <div class="row">
              <div class="col-12 col-md-12 col-lg-12">
              
            
                
                <div class="table-responsive">

     
                    <table id="report-table" class="table table-bordered table-striped mb-0" >
                    <thead>
                    <tr>
                    
                    <th>Name</th>
                    <th>Number</th>
                    <th>Address</th>
                    <th>Block/Unblock</th>
                   
                    </tr>
                    </thead>
                    <tbody>
                    


                        <% for(i=0;i<result.length;i++){ %>
                  <tr>
                    <td><%=result[i].name%></td>
                    <td><%=result[i].number%></td>
                    <td><p style="width: 200px;"><%=result[i].landmark%> ,  <%=result[i].area%> , <%=result[i].city%> , <%=result[i].pincode%>  </p></td>
                  
                    <% if(result[i].status == '' || result[i].status == [] || result[i].status == null || result[i].status == 'null' || result[i].status == 'unblock') { %>
                        <td><a href="/admin/update/agent?id=<%=result[i].id%>&status=block"><button type="button" class="btn btn-danger">Block</button></a></td>

                        <% } else {  %>
                            <td><a href="/admin/update/agent?id=<%=result[i].id%>&status=unblock"><button type="button" class="btn btn-dark">Unblock</button></a></td>

                        <% } %>
                   
                
   </tr>
<% } %>

                    </tbody>
                    </table>
                    </div>



    
    
    
   
            
              </div>
             
            </div>
          </div>
        </section>
        <div class="settingSidebar">
          <a href="javascript:void(0)" class="settingPanelToggle"> <i class="fa fa-spin fa-cog"></i>
          </a>
       
        </div>
      </div>
      
    </div>
  </div>
  <!-- General JS Scripts -->
  <script src="/light/assets/js/app.min.js"></script>   
  <!-- JS Libraies -->
  <!-- Page Specific JS File -->
  <!-- Template JS File -->
  <script src="/light/assets/js/scripts.js"></script>
  <!-- Custom JS File -->
  <script src="/light/assets/js/custom.js"></script>


  <script src="/javascripts/users.js"></script>

  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
  <script>
  
    $('.uploadImage').submit(function (e) {
      e.preventDefault();
  
      // var title = $('#title').val(); 
  
      $(this).ajaxSubmit({
        // data: {title: title},
        contentType: 'application/json',
        success: function (response) {
          alert('successfully')
        }
      });
      return false;
    });
  

    $('.transfer_status').click(function(){
      let transfer_status = $(this).attr('return1')
      let id = $(this).attr('id')
   
      $.post('/booking/update',{id,transfer_status},data=>{
        console.log(data)
        window.location.reload()
      })

    })
  
  
  </script>

</body>


</html>