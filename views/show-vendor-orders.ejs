<%- include ('./adminheader.ejs') %>
      <!-- Main Content -->
      <div class="main-content">
        <section class="section">
          <div class="section-body">
            <div class="row">
              <div class="col-12 col-md-12 col-lg-12">
              
            
                
                <div class="table-responsive">

     
                    <table id="report-table" class="table table-bordered table-striped mb-0" >
                    <thead>
                    <tr>
                    
                    <th>Name</th>
                    <th>Number</th>
                    <th>Address</th>
                    <th>Type</th>
                    <th>Brand</th>
                   
                      <th>Switch On</th>
                      <th>Processor</th>
                      <th>RAM</th>
                      <th>Hard Disk</th>
                      <th>Touch Screen</th>
                      <th>Screen Size</th>
                      <th> Graphics Card</th>
                    
                      <th>Age</th>
                      <th>Condition</th>
                      <th>Amount</th>
                      <th>Payment</th>
                      <th>Assigned</th>  

                    <th>Date</th>
                    <th>Issue</th>

<th>Trasnfered</th>

                    </tr>
                    </thead>
                    <tbody>
                    


                        <% for(i=0;i<result.length;i++){ %>
                  <tr>
                    <td><%=result[i].name%></td>
                    <td><%=result[i].number%></td>
                    <td><p style="width: 200px;"><%=result[i].address%></p></td>
                    <td><%=result[i].type%></td>
                    <td><%=result[i].brandname%> , <%=result[i].modelname%></td>
                   
                    <td> <%=result[i].switch_on%> </td>
                    <td> <%=result[i].processor%></td>
                  
                   <td> <%=result[i].ram%></td>
                
                    

<td><%=result[i].hard_disk%></td>

 <td> <%=result[i].touchscreen%></td>
 <td> <%=result[i].screen_size%></td>
  <td> <%=result[i].graphic_card%></td>


                    <td><%=result[i].age%></td>
                    <td><%=result[i].condition%></td>
                    <td><%=result[i].amount%></td>
                    <td><%=result[i].payment_method%></td>
                    <td><%=result[i].assignedname%></td>

                    <td><%=result[i].date%></td>


                    <td style="width: 800px;"><p style="width: 800px;"><%=result[i].issues.split('null,').join('\n')%></p></td>


                  
                    </tr>

<% } %>

                    </tbody>
                    </table>
                    </div>



    
    
    
   
            
              </div>
             
            </div>
          </div>
        </section>
        <div class="settingSidebar">
          <a href="javascript:void(0)" class="settingPanelToggle"> <i class="fa fa-spin fa-cog"></i>
          </a>
       
        </div>
      </div>
      
    </div>
  </div>
